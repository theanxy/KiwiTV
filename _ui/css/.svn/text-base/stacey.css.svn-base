/* page-shipping */
div#shipping { padding: 0 13px; position: relative; }
div#shipping h1 { color: #a6ba1b; font-size: 2.5em; line-height: 1.1em; font-weight: normal; padding: 14px 0 36px 18px; } 
div#shipping h2 { color: #f48027; font-size: 1.5em; line-height: 1.1em; font-weight: normal; padding: 0 0 0 18px; letter-spacing: 0.015em; } 
div#shipping label { cursor: pointer; }
div#shipping input.text, div#login input.text { border: 1px solid #6c7911; }

/* shipping-nav */
ol.shipping-nav { width: 407px; position: absolute; top: 17px; right: 18px; height: 24px; }
ol.shipping-nav li { margin-left: 21px; float: left; position: absolute; top: 0; left: 0; text-transform: uppercase; color: #85898c; font-size: 1.2em; line-height: 1.6em; text-indent: 15px; }
ol.shipping-nav li span { position: absolute; top: 0; left: 0; z-index: 10; background: url(../images/bg_login_shipping_billing_confirm.gif) no-repeat; } 
ol.shipping-nav li, ol.shipping-nav li span { display: block; height: 24px; overflow: hidden; }
ol.shipping-nav li.login { margin-left: 0; }
ol.shipping-nav li.login, ol.shipping-nav li.login span { width: 72px; }
ol.shipping-nav li.shipping, ol.shipping-nav li.shipping span { width: 96px; }
ol.shipping-nav li.billing, ol.shipping-nav li.billing span { width: 82px; }
ol.shipping-nav li.confirm, ol.shipping-nav li.confirm span { width: 93px; }
ol.shipping-nav li.login { left: 0; }
ol.shipping-nav li.shipping { left: 72px; }
ol.shipping-nav li.billing { left: 189px; }
ol.shipping-nav li.confirm { left: 292px; }   
ol.shipping-nav li.login span { background-position: 0 0; } 
ol.shipping-nav li.shipping span { background-position: -72px 0; } 
ol.shipping-nav li.billing span { background-position: -168px 0; } 
ol.shipping-nav li.confirm span { background-position: -250px 0; }  
ol.shipping-nav li.login span.current { background-position: 0 -24px; } 
ol.shipping-nav li.shipping span.current { background-position: -72px -24px; } 
ol.shipping-nav li.billing span.current { background-position: -168px -24px; } 
ol.shipping-nav li.confirm span.current { background-position: -250px -24px; } 

